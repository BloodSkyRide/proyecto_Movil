<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="proyecto_movil.Views.ReservasView"
             IconImageSource="usuario.png"
             
              Title="Lista de Usuarios" >
    <ContentPage.Content>
        <StackLayout Spacing="10">
            <Button Clicked="Button_Clicked" Text="Atras" BackgroundColor="#E74C3C">

            </Button>


            <Button x:Name="ListaUser"  Clicked="ListaUser_Clicked" Text="Listado de usuarios" />
            <Button Text="Elegir Reservas" Command="{Binding commandReservation}"/>




        </StackLayout>
    </ContentPage.Content>
</ContentPage>